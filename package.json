    "repository": {
        "type": "git",
        "url": "git+https://github.com/bischoff-m/nodecode.git"
    },
    "engines": {
        "node": ">=18.0.0",
        "npm": ">=8.0.0"
    },
    "scripts": {
        "dev": "cd packages/nodecode-ui/ && node .vite/watch.mjs",
        "build:web": "node packages/nodecode-ui/.vite/build.mjs",
        "build:win": "node packages/nodecode-ui/.vite/build.mjs && electron-builder",
        "prebuild": "node packages/nodecode-ui/.vite/prebuild.mjs",
        "docs": "npx serve packages/nodecode-ui/docs"
    },
    "workspaces": [
        "packages/nodecode-ui",
        "packages/nodecode-docs"
    ]
