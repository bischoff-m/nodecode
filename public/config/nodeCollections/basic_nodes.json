{
    "$schema": "../schemas/NodeCollection.schema.json",
    "name": "Basic Nodes",
    "nodes": [
        {
            "id": "sql_query",
            "title": "SQL Query",
            "fields": [
                {
                    "id": "in-select_out-query",
                    "type": "InputOutput",
                    "props": {
                        "inputLabel": "Select",
                        "outputLabel": "Query"
                    }
                },
                {
                    "id": "in-from_out-columns",
                    "type": "InputOutput",
                    "props": {
                        "inputLabel": "From",
                        "outputLabel": "Columns"
                    }
                },
                {
                    "id": "in-groupby",
                    "type": "InputOutput",
                    "props": {
                        "inputLabel": "Group by"
                    }
                },
                {
                    "id": "in-orderby",
                    "type": "InputOutput",
                    "props": {
                        "inputLabel": "Order by"
                    }
                },
                {
                    "id": "in-where",
                    "type": "InputOutput",
                    "props": {
                        "inputLabel": "Where"
                    }
                },
                {
                    "id": "in-having",
                    "type": "InputOutput",
                    "props": {
                        "inputLabel": "Having"
                    }
                }
            ]
        },
        {
            "id": "sql_aggregate",
            "title": "Aggregate",
            "fields": [
                {
                    "id": "input-output",
                    "type": "InputOutput",
                    "props": {
                        "inputLabel": "Columns",
                        "outputLabel": "Columns"
                    }
                },
                {
                    "id": "select",
                    "type": "Select",
                    "props": {
                        "values": [
                            "Count",
                            "Sum",
                            "Min",
                            "Max"
                        ],
                        "default": "Count"
                    }
                }
            ]
        },
        {
            "id": "sql_distinct",
            "title": "Distinct",
            "fields": [
                {
                    "id": "in-columns_out-columns",
                    "type": "InputOutput",
                    "props": {
                        "inputLabel": "Columns",
                        "outputLabel": "Columns"
                    }
                }
            ]
        },
        {
            "id": "input_list",
            "title": "List Input",
            "fields": [
                {
                    "id": "output",
                    "type": "InputOutput",
                    "props": {
                        "outputLabel": "List",
                        "outputDatatype": "array"
                    }
                },
                {
                    "id": "datatype",
                    "type": "Select",
                    "props": {
                        "label": "Datatype",
                        "values": [
                            "String",
                            "Int",
                            "Float",
                            "Boolean"
                        ],
                        "default": "String"
                    }
                },
                {
                    "id": "item_list",
                    "type": "List",
                    "props": {
                        "label": "Items",
                        "placeholder": "Add item"
                    }
                }
            ]
        },
        {
            "id": "output",
            "title": "Output",
            "fields": [
                {
                    "id": "input",
                    "type": "InputOutput",
                    "props": {
                        "inputLabel": "Output",
                        "inputDatatype": "any",
                        "inputMulti": true
                    }
                }
            ]
        },
        {
            "id": "sql_mysql-table",
            "title": "MySQL Table",
            "fields": [
                {
                    "id": "output",
                    "type": "InputOutput",
                    "props": {
                        "outputLabel": "Table"
                    }
                },
                {
                    "id": "server",
                    "type": "Select",
                    "props": {
                        "label": "Server",
                        "values": [
                            "i11 MariaDB Server"
                        ],
                        "default": "i11 MariaDB Server"
                    }
                },
                {
                    "id": "database",
                    "type": "Select",
                    "props": {
                        "label": "Database",
                        "values": [
                            "Mimic4",
                            "Mimic3"
                        ],
                        "default": "Mimic4"
                    }
                },
                {
                    "id": "table",
                    "type": "Radio",
                    "props": {
                        "label": "Table",
                        "valueLabelPairs": {
                            "admissions": "Admissions",
                            "chartevents": "Chart Events",
                            "icustays": "ICU Stays",
                            "labevents": "Lab Events",
                            "microbiologyevents": "Microbiology Events",
                            "observations": "Observations",
                            "procedures": "Procedures",
                            "procedureevents": "Procedure Events",
                            "prescriptions": "Prescriptions",
                            "readmissions": "Readmissions",
                            "transfers": "Transfers",
                            "vitals": "Vitals"
                        },
                        "defaultValue": "chartevents"
                    }
                }
            ]
        },
        {
            "id": "sql_column-select",
            "title": "Column Select",
            "fields": [
                {
                    "id": "inputoutput",
                    "type": "InputOutput",
                    "props": {
                        "inputLabel": "Table",
                        "outputLabel": "Columns"
                    }
                },
                {
                    "id": "multiselect",
                    "type": "MultiSelect",
                    "props": {
                        "placeholder": "Add column",
                        "valueLabelPairs": {
                            "admissions": "Admissions",
                            "chartevents": "Chart Events",
                            "icustays": "ICU Stays",
                            "labevents": "Lab Events",
                            "microbiologyevents": "Microbiology Events",
                            "observations": "Observations",
                            "procedures": "Procedures",
                            "procedureevents": "Procedure Events",
                            "prescriptions": "Prescriptions",
                            "readmissions": "Readmissions",
                            "transfers": "Transfers",
                            "vitals": "Vitals"
                        }
                    }
                }
            ]
        }
    ]
}